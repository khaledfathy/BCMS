@using BCMS.Models;
@model Market
<link href="~/Areas/Admin/css/MyStyle.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        $('.ui-dialog-buttonset button').addClass('btn btn-primary12 btn-sm');
    });
</script>

<div style=" text-align: center;width:82%">
    <div class="box col-md-8" style="width:121%;text-align: center">
        <div class="box-content" style="width:100%;">
            <div class="form-group">
                <label for="exampleInputEmail1">السوق باللغة العربية</label>
                <input name="ActivityArName" class="form-control" type="text" disabled="disabled" value="@Model.MarketArName" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">السوق باللغة الإنجليزية</label>
                <input name="ActivityEnName" class="form-control" type="text" disabled="disabled" value="@Model.MarketEnName" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">الدولة</label>
                <input name="ActivityCode" class="form-control" type="text" disabled="disabled" value="@Model.Country.CountryArName" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">نوع السوق</label>
                <input name="ActivityCode" class="form-control" type="text" disabled="disabled" value="@Model.MarketType.MarketTypeArName" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">سنة التأسيس</label>
                <input name="ActivityCode" class="form-control" type="text" disabled="disabled" value="@Model.EstablishedYear" />
            </div>
        </div>
    </div>
</div>
