@using BCMS.Models;
@model Elder
<link href="~/Areas/Admin/css/MyStyle.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        $('.ui-dialog-buttonset button').addClass('btn btn-primary12 btn-sm');
    });
</script>
<div style=" text-align: center;width:82%">
    <div class="box col-md-8" style="width:121%;text-align: center">
        <div class="box-content" style="width:100%;">
            <div class="form-group">
                <label for="exampleInputEmail1">كود المؤلف</label>
                <input name="SectorID" class="form-control" type="text" disabled="disabled" value="@Model.EldersID" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">إسم المؤلف</label>
                <input name="SectorName" class="form-control" type="text" disabled="disabled" value="@Model.EldersName" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">الوصف</label>
                <textarea name="AdviceContent" class="form-control" disabled="disabled">@Model.EldersDescription</textarea>
            </div>
        </div>
    </div>
</div>
