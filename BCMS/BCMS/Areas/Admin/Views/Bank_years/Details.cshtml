@using BCMS.Models;
@model Bank_Year
<link href="~/Areas/Admin/css/MyStyle.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        $('.ui-dialog-buttonset button').addClass('btn btn-primary12 btn-sm');
    });
</script>

<div style=" text-align: center;width:82%">
    <div class="box col-md-8" style="width:121%;text-align: center">
        <div class="box-content" style="width:100%;">
            <div class="form-group">
                <label for="exampleInputEmail1">البنك</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.BankId" />
            </div>

            <div class="form-group">
                <label for="exampleInputEmail1">السنة</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.YearId" />
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">عدد الـ ATM</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.ATMMachine" />
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">نسبة نمو الـ ATM</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.PercentageGrowthATMMachine" />
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">عدد فروع البنك</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.BranchsBank" />
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">نسبة نمو فروع البنك</label>
                <input name="YearID" class="form-control" type="text" disabled="disabled" value="@Model.PercentageGrowthBranchsBank" />
            </div>
        </div>
    </div>
</div>