@{
    ViewBag.Title = "StockUp";
    Layout = null;
}
<link href="~/Materials/UTMS/css/taps.css" rel="stylesheet" />
@*<script type="text/javascript">
        $(document).ready(function() {
            $('#tab1').click(function () {
                location.reload();
            });
        });
</script>*@
<div>
    <div class="large-12 columns">
        <div class="large-3 column">
         @Html.Partial("_RightPartialStockUp")
        </div>
        <div class="large-6 column">
            <div class="TikerChartWrapper">
                @Html.Partial("_TopMiddlePartialStockUp")
                <!--START-->
                <style>

                    /*Third*/
                    .TabsArticle {
                        float: none;
                        list-style: none;
                        position: relative;
                        padding: 0;
                        margin: 0px 0 0 0px;
                        /*border-bottom: 1px solid #333;*/
                        min-height: 330px;
                        /*height: initial;*/
                    }

                        .TabsArticle ul {
                            padding: 0;
                            margin: 0px 0 0 0px;
                        }

                        .TabsArticle li {
                            padding: 0 !important;
                            margin: 0px 1px 0 0px !important;
                            float: right !important;
                            display: inline-block !important;
                        }

                        .TabsArticle input[type="radio"] {
                            position: absolute;
                            top: -9999px;
                            right: -9999px;
                        }

                        .TabsArticle label {
                            display: block;
                            font-size: 12px;
                            font-weight: normal;
                            color: #000;
                            text-decoration: none;
                            background-color: #6d6d6d;
                            cursor: pointer;
                            position: relative;
                            top: 4px;
                            padding: 6px 7px;
                            margin: 0px 0 0 0px !important;
                            border-radius: 2px 2px 0 0;
                            -moz-transition: all 0.2s ease-in-out;
                            -o-transition: all 0.2s ease-in-out;
                            -webkit-transition: all 0.2s ease-in-out;
                            transition: all 0.2s ease-in-out;
                        }

                            .TabsArticle label:hover {
                                background: #ffff00;
                            }

                        .TabsArticle .tab-contentArticle {
                            font-family: Tahoma;
                            font-size: 12px;
                            z-index: 2;
                            display: none;
                            overflow: hidden;
                            width: 100%;
                            line-height: normal;
                            padding: 10px;
                            position: absolute;
                            top: 31px;
                            right: 0;
                            color: #FFF;
                            background: #2d2d2d;
                        }

                        .TabsArticle [id^="tabArticle"]:checked + label {
                            top: 0;
                            padding-top: 10px;
                            background: #92d050;
                        }

                        .TabsArticle [id^="tabArticle"]:checked ~ [id^="tab-contentArticle"] {
                            display: block;
                            -moz-transition: all 0.2s ease-in-out;
                            -o-transition: all 0.2s ease-in-out;
                            -webkit-transition: all 0.2s ease-in-out;
                            transition: all 0.2s ease-in-out;
                        }
                </style>
                @Html.Partial("_NewsTabPartialStockUp")
                <!--End-->
                <div class="Clear H15"></div>
                <hr />
                <!--START-->
                @Html.Partial("_StockTabPartialStockUp")
                <!--End-->
              <!-- here-->!>
            </div>
</div>
        @Html.Partial("_LeftPartialStockUp")
    </div>
</div>
