    Layout = null;
}

<!DOCTYPE html>
<html dir="rtl" lang="ar">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>BorsaCapital | Welcome</title>

    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Materials/Portfolio/Images/favicon.ico" type="image/x-icon">
    <script src="~/Materials/Portfolio/js/lodash.js"></script>
    <link href="~/Materials/Portfolio/css/taps.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/css/foundation.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/css/MainStyle.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/css/Inputs.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/css/Css3.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/css/Icons.css" rel="stylesheet" />
    <link href="~/Materials/Portfolio/webicons-master/webicons.css" rel="stylesheet" />
    <script src="~/Materials/Portfolio/js/jquery.min.js"></script>
    <script src="~/Materials/Portfolio/js/foundation.min.js"></script>
    <script src="~/Materials/Portfolio/js/spin.min.js"></script>
    <script src="~/Materials/Portfolio/Charts/angular-google-maps.js"></script>

    <style>
        .angular-google-map-container {
            height: 400px;
        }
    </style>

</head>
<body>
    <div class="BGWrapper">
        <!-- Start Header-->
        <div class="clear"></div>
        <div class="MainMenuWrappe">
            <div class="large-12 columns">
                <div class="MainLogo"> <a class="right" href="#BorsaCloud"><img src="~/Materials/Portfolio/Images/Logo.png" /></a><h2 class="right">المحفظة التجريبية</h2></div>
                <div class="left">
                    <h4>
                        <span class="color4">مرحبا</span>
                        خالد الطويل
                    </h4>
                </div>
                <div class="left">&nbsp;|&nbsp;</div>
                <a href="" class="left NormalLink LoginNav"> تسجيل خروج</a>
            </div>
        </div>

        <div class="clearfix"></div>
        <!--End Header-->
        <div class="MAINmainWrapper">
            <!--START CONTENT-->
            <div class="MainWrapperCenter">
                <!--START-->
                <div class="large-12 columns">

                    <div class="large-12 columns">
                        <div style="text-align: left"><a class="myButton" href="@Url.Action("Index", "Home", new { Area = "Portfolio" })">عوده</a></div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="clearfix"><div class="H5"></div></div>
                    <div class="large-12 columns">
                        <div class="tabs-wrapper">
                            <input type="radio" name="tab" id="tab1" class="tab-head" checked="checked" />
                            <label for="tab1">تحليل فني</label>
                            <input type="radio" name="tab" id="tab2" class="tab-head" />
                            <label for="tab2">تحليل اساسي</label>
                            <input type="radio" name="tab" id="tab3" class="tab-head" />
                            <label for="tab3">تحليل استراتيجي</label>
                            <div class="tab-body-wrapper">
                                <div id="tab-body-1" class="tab-body">
                                    <h1>تحليل فني</h1>
                                    <p>الهدف الأساس من إنشاء شركة بورصة كابيتال هو تقديم خدمة المشورة للمتداولين في سوق الأسهم السعودية عن طريق موقع الكتروني متقدم سهل التصفح يخدم جميع فئات المتداولين مبتدئين كانوا أم محترفين.</p>
                                    <div class="ChartList">
                                        <ul>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                        </ul>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                                <div id="tab-body-2" class="tab-body">
                                    <h1>تحليل اساسي</h1>
                                    <p>الهدف الأساس من إنشاء شركة بورصة كابيتال هو تقديم خدمة المشورة للمتداولين في سوق الأسهم السعودية عن طريق موقع الكتروني متقدم سهل التصفح يخدم جميع فئات المتداولين مبتدئين كانوا أم محترفين.</p>
                                    <div class="ChartList">
                                        <ul>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                        </ul>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                                <div id="tab-body-3" class="tab-body">
                                    <h1>تحليل استراتيجي</h1>
                                    <p>الهدف الأساس من إنشاء شركة بورصة كابيتال هو تقديم خدمة المشورة للمتداولين في سوق الأسهم السعودية عن طريق موقع الكتروني متقدم سهل التصفح يخدم جميع فئات المتداولين مبتدئين كانوا أم محترفين.</p>
                                    <div class="ChartList">
                                        <ul>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                            <li><div><img src="~/Materials/Portfolio/Images/Chart.jpg" style="width: 100%;" /></div></li>
                                        </ul>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--END-->
                <div class="clearfix"></div>
                <!--END CONTENT-->
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
        <div class="FooterWrapperBG">
            <div class="copyRight">حقوق النشر © BorsaCapital 2015</div>
            <div class="clear"></div>
        </div>
    </div>
    <script type="text/javascript">
        $(function () {

            $(".btnMusic").click(function () {
                window.open("/test/Home/Music", "", "width=500, height=600");
            });

            $("#InfoBar").click(function () {
                $("#NewsBar").slideToggle("slow");
            });

            $("#alert").click(function () {
                $("#Alerts").slideToggle("slow");
            });

            var $elem = $('#allBody');

            $(".Fbutton").click(function (event) {
                event.preventDefault();
                $("#FooterDetails").slideToggle(500);
                $('html, body').animate({ scrollTop: $elem.height() }, 500);
            });
        });

    </script>
    <script>
        $(document).foundation();
    </script>
</body>
</html>




